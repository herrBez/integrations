metricsets: ["wmi"]
data_stream:
  dataset: {{data_stream.dataset}}
period: {{period}}
wmi:
  namespace: {{wmi.namespace}}
  queries: {{wmi.queries}}
  warning_threashold: {{wmi.warning_threshold}}
  include_queries: {{wmi.include_queries}}
  include_null_properties: {{wmi.include_null}}
  include_empty_strings_properties: {{wmi.include_empty_strings}}
{{#if processors}}
processors:
{{processors}}
{{/if}}

{{#each tags as |tag|}}
  - {{tag}}
{{/each}}